<Page
    x:Class="MetroExplorer.Pages.GuidePage.PageUserGuide"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Background="{StaticResource ApplicationPageBackgroundThemeBrush}">
        <Button x:Name="Button_ToGuidPage2" Click="Button_ToGuidPage2_Click"
						x:Uid="Uid_Button_ToGuidPage2"
						Margin="5,0" HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button.Style>
                <Style TargetType="Button" BasedOn="{StaticResource AppBarButtonStyle}">
                    <Setter Property="AutomationProperties.Name" Value=""/>
                    <Setter Property="Content" Value="&#xE111;"/>
                </Style>
            </Button.Style>
        </Button>
        <Button x:Name="Button_ToMainPage" Click="Button_ToMainPage_Click"
						x:Uid="Uid_Button_ToMainPage"
						Margin="150,0,0,0" HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button.Style>
                <Style TargetType="Button" BasedOn="{StaticResource AppBarButtonStyle}">
                    <Setter Property="AutomationProperties.Name" Value=""/>
                    <Setter Property="Content" Value="&#xE10F;"/>
                </Style>
            </Button.Style>
        </Button>
    </Grid>
</Page>